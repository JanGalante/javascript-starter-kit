{
    "name": "javascript-development-environment",
    "version": "1.0.0",
    "description": "Starter kit for JavaScript development environment",
    "scripts": {
        "prestart": "babel-node buildScripts/startMessage.js",
        "start": "npm-run-all --parallel security-check start-server",
        "security-check": "nsp check",
        "start-server": "http-server src -p 3000 -o",
        "localtunnel": "lt --port 3000 --subdomain donquijote",
        "share": "npm-run-all --parallel start-server localtunnel"
    },
    "author": "Jan Lundholm Galante",
    "license": "MIT",
    "devDependencies": {
        "babel-cli": "6.16.0",
        "babel-core": "6.17.0",
        "babel-loader": "6.2.5",
        "babel-preset-latest": "6.16.0",
        "babel-register": "6.16.3",
        "chalk": "1.1.3",
        "express": "4.14.0",
        "http-server": "*",
        "localtunnel": "1.8.1",
        "npm-run-all": "3.1.1",
        "nsp": "2.6.2"
    }
}